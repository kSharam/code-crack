package com.company;

public class isUnique {
    public boolean isUnique(String name)
    {
        String toCheck = name;
        for(int i=0; i<toCheck.length(); i++) {
            {
                if(i == toCheck.length()-1){ return true;}
               
               for(int j= i+1; j< toCheck.length(); j++)
                {
                    if(toCheck.charAt(i) == toCheck.charAt(j))
                    {
                        return false;}
                }

            }
        }
        return true;
    }
}
